{
  "name": "@mrwconsulting/hello-cdk",
  "version": "0.0.0",
  "description": "First AWS CDK App",
  "author": {
    "name": "MRWConsulting",
    "email": "mark@mrwconsulting.tech",
    "organization": false
  },
  "license": "ISC",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "hello-cdk": "bin/hello-cdk.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrwconsulting/cdk-monorepo.git"
  },
  "publishConfig": {
    "registry": "https://github.com/mrwconsulting"
  },
  "scripts": {
    "build": "jsii",
    "clean": "rimraf .jsii tsconfig.json */*.js cdk.out node_modules",
    "deploy": "cdk deploy",
    "package": "jsii-pacmak",
    "synth": "cdk synth"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "mrwconsulting.hello-cdk",
        "module": "mrwconsulting.hello_cdk"
      },
      "java": {
        "package": "mrwconsulting.hello-cdk",
        "maven": {
          "groupId": "mrwconsulting",
          "artifactId": "hello-cdk"
        }
      }
    },
    "projectReferences": true,
    "metadata": {
      "jsii": {
        "rosetta": {
          "strict": true
        }
      }
    }
  },
  "bundledDependencies": [
    "source-map-support"
  ],
  "devDependencies": {
    "@types/jest": "27.5.0",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "aws-cdk-lib": "2.25.0",
    "constructs": "10.1.24",
    "jest": "27.5.1",
    "ts-jest": "27.1.4",
    "aws-cdk": "2.25.0",
    "ts-node": "10.7.0",
    "typescript": "3.9.7"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.25.0",
    "constructs": "10.1.24"
  },
  "dependencies": {
    "source-map-support": "0.5.21"
  }
}
